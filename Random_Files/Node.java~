public class Node{
    private String data;
    private Node next;
    public Node(String s){
	data = s;
    }
    public void setData(String s){
	data = s;
    }
    public String getData(){
	return data;
    }
    public void setNext(Next n){
	next = n;
    }
    public next getNext(){
	return next;
    }
}
